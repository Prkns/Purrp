  "tokenColors": [
    // Twig //
    {
      "name": "Twig Italic",
      "scope": "meta.tag.template.block.twig",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Twig Open",
      "scope": "punctuation.section.tag.twig, keyword.control.twig, punctuation.separator.property.twig, punctuation.definition.string.begin.twig, punctuation.definition.string.end.twig, punctuation.definition.parameters.begin.twig, punctuation.definition.parameters.end.twig",
      "settings": {
        "foreground": "#ae95bd"
      }
    },
    {
      "name": "Twig Text",
      "scope": "variable.other.twig, meta.tag.template.value.twig, variable.other.property.twig, string.quoted.double.twig, string.quoted.single.twig",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Twig Operators",
      "scope": "keyword.operator.assignment.twig, constant.numeric.twig, keyword.operator.comparison.twig",
      "settings": {
        "foreground": "#b174f7"
      }
    },

    // SASS/SCSS/CSS
    {
      "name": "SASS Include/Extend/Import @ declarations",
      "scope": "entity.name.tag.reference.scss",
      "settings": {
        "foreground": "#ba82ffff"
      }
    },
    {
      "name": "Class selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.name.tag.css",
        "entity.name.tag.wildcard.scss",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#82ffd3ff"
      }
    },
    {
      "name": "Class selectors",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#82ffd392"
      }
    },
    {
      "name": "Single quoted strings",
      "scope": "string.quoted.single.scss, string.quoted.double.scss",
      "settings": {
        "foreground": "#d391da6f"
      }
    },
    {
      "name": "Pseudo selectors",
      "scope": "entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#f7c5ff"
      }
    },
    {
      "name": "Color declarations",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comma after declarations",
      "scope": [
        "punctuation.seperator.key-value.scss",
        "punctuation.terminator.rule.scss"
      ],
      "settings": {
        "foreground": "#ffffff76"
      }
    },
    {
      "name": "SASS 'content' declaration",
      "scope": [
        "entity.name.function.scss",
        "entity.other.attribute-name.placeholder.css"
      ],
      "settings": {
        "foreground": "#e98cf7e4"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.terminator.rule.css",
        "punctuation.definition.string.begin.scss",
        "punctuation.definition.string.end.scss",
        "punctuation.definition.parameters.begin.bracket.round.scss",
        "punctuation.definition.parameters.end.bracket.round.scss",
        "punctuation.separator.key-value.scss",
        "punctuation.terminator.rule.scss",
        "punctuation.definition.begin.bracket.round.scss",
        "punctuation.definition.end.bracket.round.scss",
        "punctuation.separator.delimiter.scss"
      ],
      "settings": {
        "foreground": "#ffffff57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation - Second Level",
      "scope": ["punctuation.section.function.scss"],
      "settings": {
        "foreground": "#ff0099b2"
      }
    },
    {
      "name": "Maths/Keyword Operators",
      "scope": ["keyword.operator.css"],
      "settings": {
        "foreground": "#ffbd83"
      }
    },
    {
      "name": "@ rule properties",
      "scope": [
        //"meta.at-rule.include.scss",
        //"meta.at-rule.extend.scss",
        //"meta.at-rule.import.scss",
        "keyword.control.at-rule.include.scss",
        "keyword.control.at-rule.extend.scss",
        "keyword.control.at-rule.import.scss",
        "keyword.control.at-rule.mixin.scss"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dda4f96b"
      }
    },
    {
      "name": "Declarations of anything other than classes or id's",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "entity.name.function.scss"
      ],
      "settings": {
        "foreground": "#de9cff"
      }
    },

    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#ffffff31",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#82ffd3"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, keyword.other.unit.px.css, keyword.other.unit.vh.css, keyword.other.unit.ms.css, keyword.other.unit.rem.css, keyword.other.unit.percentage.css",
      "settings": {
        "foreground": "#82f8fc"
      }
    },
    // --- //
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "URL",
      "scope": "*url*, *link*, *uri*",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#c389d5"
      }
    },
    {
      "name": "Markup tag",
      "scope": "meta.tag, declaration.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#e15d97"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#ff81e6"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7e718e"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#35ba66"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#35ba66"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#bb8ee7"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#e15d97"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#35ba66"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#0aacc5"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#6be5fd"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": " italic",
        "foreground": "#00d5ff"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
